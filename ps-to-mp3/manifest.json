{
  "name": "ps-to-mp3",
  "short_name": "ps-to-mp3",
  "start_url": "/index.html",
  "display": "standalone",
  "description": "A web app to convert audio files to MP3 with maximum quality.",
  "icons": [
    {
      "src": "/favicon.svg",
      "type": "image/svg+xml"
    }
  ],
  "file_handlers": [
    {
      "action": "/index.html",
      "accept": {
        "audio/*": [".wav", ".mp3", ".ogg", ".flac", ".aac", ".m4a"]
      }
    }
  ]
}
